<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:layout="sap.ui.layout">
	<layout:HorizontalLayout >
			<Button
				text="{$this>buttonAddText}"
				type="Transparent"
				enabled="{$this>enabledAdd}"
				press="onAddPress" />
			<Button
				text= "{$this>buttonDeleteText}"
				type="Transparent"
				enabled="{$this>enabledDelete}"
				press="onDeletePress" />
	</layout:HorizontalLayout>
</core:FragmentDefinition>