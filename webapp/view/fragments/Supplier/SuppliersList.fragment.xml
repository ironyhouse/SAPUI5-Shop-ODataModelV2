<core:FragmentDefinition
	height="100%"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:f="sap.f">
 	<List
	 	id="SupplierProductList"
		items="{Products}"
		headerText="{i18n>productListTitle}">
		<ObjectListItem
			type="Active"
			title="{Name}"
			number="{Price}"
			numberUnit="{i18n>usd}"
			numberState="Success" >
		</ObjectListItem>
	</List>
</core:FragmentDefinition>